{
    "name": "sdra-deploy",
    "source": {
        "type": "CODEPIPELINE",
        "buildspec": "AWS-CICD/CodeBuild/build-specs/buildspec-deploy.yml",
        "insecureSsl": false
    },
    "artifacts": {
        "type": "CODEPIPELINE",
        "name": "sdra-deploy",
        "packaging": "NONE",
        "encryptionDisabled": false
    },
    "cache": {
        "type": "NO_CACHE"
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/amazonlinux2-x86_64-standard:3.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "environmentVariables": [],
        "privilegedMode": false,
        "imagePullCredentialsType": "CODEBUILD"
    },
    "serviceRole": "<code_build_deploy_service_role_arn>",
    "timeoutInMinutes": 60,
    "queuedTimeoutInMinutes": 480,
    "logsConfig": {
        "cloudWatchLogs": {
            "status": "ENABLED"
        },
        "s3Logs": {
            "status": "DISABLED",
            "encryptionDisabled": false
        }
    }
}